    <connection>con_sisflora</connection>
    <execute_each_row>N</execute_each_row>
    <limit>0</limit>
    <sql>-- EMPREENDIMENTOS
SELECT
    e.ID,
    e.NOME,
    NVL(e.CNPJ, e.CPF) AS CPF_CNPJ,
    e.LATITUDE_ENTRADA_GDEC,
    e.LONGITUDE_ENTRADA_GDEC,
    m.TEXTO MUNICIPIO_ATIVIDADE,
    REGEXP_REPLACE(e.CLASSIFICACAO, '\[|\]', '') AS TIPO,
    REGEXP_REPLACE(e.SITUACAO, '(\[|\])', '') SITUACAO,
    s.DATA_CADASTRO,
    s.DATA_ENVIO, 
	s.TIPO_OPERACAO,
    CASE 
    	WHEN s.STATUS_REQUISICAO = 0 THEN 'Não enviado'	
    	WHEN s.STATUS_REQUISICAO = 1 THEN 'Enviado e salvo/alterado/deletado com sucesso'
    	WHEN s.STATUS_REQUISICAO = 2 THEN 'Enviado e com erros'
    END AS STATUS_ENVIO,
    s.MENSAGEM_SINAFLOR AS "Log"
FROM sisflora2.CONTROLE_EMP_SINAFLOR s
JOIN (
    SELECT MAX(ID) AS ID
    FROM sisflora2.CONTROLE_EMP_SINAFLOR
    GROUP BY EMPREENDIMENTO_ID
) g ON g.ID = s.ID
JOIN sisflora2.EMPREENDIMENTO e ON e.ID = s.EMPREENDIMENTO_ID
JOIN sisflora2.MUNICIPIO m ON e.ENDERECO_ATIV_MUNICIP_ID = m.ID
WHERE 1 = 1
-- AND s.STATUS_REQUISICAO = 2
  AND s.TIPO_OPERACAO != 'DELETE'
ORDER BY e.ID</sql>

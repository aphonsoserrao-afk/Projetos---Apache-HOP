  <transform>
    <name>consulta_f_sinaflor_aut_credito</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>con_sisflora</connection>
    <execute_each_row>N</execute_each_row>
    <limit>0</limit>
    <sql>SELECT
	s.id id_contr_aut_cred,
	ac.ID id_aut,
    ac.EMPREENDIMENTO_ID AS CEPROF,
    e.nome AS EMPREENDIMENTO,
    e.ie,
    ac.NUMERO AS NUMERO,
    ac.NUMERO_PROCESSO AS PROCESSO,
    REGEXP_REPLACE(ac.TIPO_CREDITO || ' - ' || ac.SUBTIPO_CREDITO, '\[|\]', '') AS TIPO,
    REGEXP_REPLACE(ac.SITUACAO, '\[|\]', '') AS SITUACAO,
    s.DATA_CADASTRO,
    s.DATA_ENVIO,
    DECODE(LOWER(s.tipo_operacao), 'post', 'Cadastro', 'put', 'Atualização', 'delete', 'Exclusão', 'Desconhecido') AS DESC_TIPO_OPERACAO,
    DECODE(s.status_requisicao, 1, 'Sucesso', 2, 'Erro', 'Desconhecido') AS DESC_STATUS_REQUISICAO,
    NVL(DBMS_LOB.SUBSTR(s.mensagem_sinaflor, 4000), 'Erro Interno') AS MENSAGEM_SINAFLOR,
    LISTAGG(msgs.message, ' | ') WITHIN GROUP (ORDER BY msgs.lvl) AS MENSAGEM_SUBERROS,
    LISTAGG(msgs.campo, ' | ') WITHIN GROUP (ORDER BY msgs.lvl) AS CAMPOS_SUBERROS
FROM sisflora2.CONTROLE_AUT_CREDITO_SINAFLOR s
JOIN (
    SELECT MAX(ID) AS ID
    FROM sisflora2.CONTROLE_AUT_CREDITO_SINAFLOR
    GROUP BY AUTORIZACAO_CREDITO_ID
) g ON g.ID = s.ID
JOIN sisflora2.AUTORIZACAO_CREDITO ac ON ac.ID = s.AUTORIZACAO_CREDITO_ID
JOIN sisflora2.EMPREENDIMENTO e ON e.id = ac.empreendimento_id
LEFT JOIN (
    SELECT
        id,
        LEVEL AS lvl,
        REGEXP_SUBSTR(DBMS_LOB.SUBSTR(mensagem_sinaflor, 4000), '"message"\s*:\s*"([^"]+)"', 1, LEVEL, NULL, 1) AS message,
        REGEXP_SUBSTR(DBMS_LOB.SUBSTR(mensagem_sinaflor, 4000), '"campo"\s*:\s*"([^"]+)"', 1, LEVEL, NULL, 1) AS campo
    FROM sisflora2.CONTROLE_AUT_CREDITO_SINAFLOR
    CONNECT BY PRIOR id = id AND LEVEL &lt;= REGEXP_COUNT(DBMS_LOB.SUBSTR(mensagem_sinaflor, 4000), '"message"\s*:\s*"')
          AND PRIOR DBMS_RANDOM.VALUE IS NOT NULL
) msgs ON msgs.id = s.id
WHERE 1 = 1
  -- AND s.STATUS_REQUISICAO = 2
  AND s.TIPO_OPERACAO != 'DELETE'
GROUP BY
	s.id,
    ac.EMPREENDIMENTO_ID,
    e.nome,
    e.ie,
    ac.NUMERO,
    ac.NUMERO_PROCESSO,
    ac.TIPO_CREDITO,
    ac.SUBTIPO_CREDITO,
    ac.SITUACAO,
    s.DATA_CADASTRO,
    s.data_envio,
    s.tipo_operacao,
    s.status_requisicao,
    DBMS_LOB.SUBSTR(s.mensagem_sinaflor, 4000),
    ac.ID
ORDER BY TO_NUMBER(NULLIF(REGEXP_SUBSTR(ac.EMPREENDIMENTO_ID, '\d+'), '')), ac.ID</sql>
